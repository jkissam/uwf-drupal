name = Unwritten Future Child Theme
description = Example child theme of Unwritten Future Theme
package = Core
version = 1.0-beta
core = 7.x
base theme = uwf
engine = phptemplate
stylesheets[screen, projection][] = css/child.css
scripts[] = js/script.js

regions[header]         = Header
regions[navigation]     = Navigation
regions[highlighted]    = Highlighted
regions[content]        = Content
regions[sidebar_first]  = First Sidebar
regions[sidebar_second] = Second Sidebar
regions[scrolling]		= Scrolling
regions[utility]		= Utility
regions[footer]         = Footer
regions[modals]			= Modals
regions[page_top]       = Page top
regions[page_bottom]    = Page bottom

; default settings
settings[uwf_navigation_title] = 'Navigation'
settings[uwf_navigation_icon] = ''
settings[uwf_navigation_mobile_break_point] = 768
settings[uwf_navigation_mobile_menu_direction] = 'left'
settings[uwf_section_navigation_selector] = '.section-navigation'
settings[uwf_section_navigation_padding] = 20
settings[uwf_external_links] = 1
settings[uwf_external_links_exceptions] = ''
settings[uwf_load_fontawesome] = 0
settings[uwf_google_webfonts] = ''
settings[uwf_custom_css] = ''
settings[uwf_validate_forms] = 1
settings[uwf_fix_footer] = 1
settings[uwf_fix_secondary] = 0
settings[uwf_fix_secondary_top] = 20
settings[uwf_fix_secondary_max] = 20
settings[uwf_fix_secondary_break_point] = 768
settings[uwf_shorten_links] = 1
settings[uwf_shorten_links_selector] = 'a'
settings[uwf_custom_js] = ''
settings[uwf_on_this_page] = 0
settings[uwf_on_this_page_heading] = 'h2'
settings[uwf_on_this_page_nav] = '.on-this-page'
settings[uwf_on_this_page_minimum_sections] = 2
settings[uwf_on_this_page_content] = '#block-system-main .field-name-body'
settings[uwf_twitter_handle] = ''
settings[uwf_twitter_hashtag] = ''
settings[uwf_twitter_field] = ''
settings[uwf_block_icons] = ''
settings[uwf_footer_block_classes] = ''
settings[uwf_region_classes_content] = 'col-xs-12 col-sm-8 col-md-6|col-xs-12 col-sm-8|col-xs-12 col-sm-8|col-xs-12'
settings[uwf_region_classes_sidebar_first] = 'col-xs-12 col-sm-4 col-md-3|col-xs-12 col-sm-4'
settings[uwf_region_classes_sidebar_second] = 'col-xs-12 col-sm-12 col-md-3|col-xs-12 col-sm-4'
settings[uwf_submitted_text] = 'Published by !username on !datetime'
settings[uwf_google_site_verification] = ''